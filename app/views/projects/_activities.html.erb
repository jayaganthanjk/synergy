<div class="container">
<div class="page-header">
<h1>Activities</h1>	
</div>
<div class="role">
<% if activities.any? %>
	<% activities.each do |activity| %>
	  <% if activity.owner.email? %>
	    <div class="activity"><%= render_activity activity %></div>
	  <% end %>
	<% end %>
<% else %>
	<div>No Activities :(</div>
<% end %>
</div>
</div>