<div class="row">
  <div class="container">
    <div class="col-md-4 col-md-offset-4">

      <%= form_tag(addUser_project_path(),method: "post") do %>
        <div class="form-group">
          <%= label_tag(:email, "Email:") %>
          <%= text_field_tag(:email,nil,{class:"form-control",autocomplete: 'off'}) %>
        </div>
        <div class="form-group">
          <%= select_tag(:role, options_for_select([['admin'],['developer'],['client']]) , :class => "form-control" ) %>
        </div>
        <%= submit_tag("Add Role",{class: "btn btn-primary"}) %>
      <% end %>
    </div>
  </div>
</div>
